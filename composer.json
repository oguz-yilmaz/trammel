{
    "name": "oguz-yilmaz/tremmel",
    "description": "Provides distinc exception handlers for Laravel",
    "keywords": [
        "tremmel",
        "laravel-exceptions",
        "exceptions",
        "handler",
        "laravel"
    ],
    "homepage": "https://github.com/oguz-yilmaz/tremmel",
    "license": "MIT",
    "authors": [
        {
            "name": "Oguz Yilmaz",
            "email": "oguz.yilmaz@yahoo.com",
            "homepage": "https://github.com/oguz-yilmaz",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel/framework": "^9.17"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "Oguz\\Tremmel\\": "src"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "test": "vendor/bin/phpunit"
    },
    "autoload-dev": {
        "psr-4": {
            "Oguz\\Tremmel\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
